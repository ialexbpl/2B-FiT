{
  "expo": {
    "name": "2B FiT",
    "slug": "2b-fit",
    "scheme": "to-be-fit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "updates": {
      "url": "",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.axel2004b.x2bfit"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-sqlite",
      "expo-web-browser",
       [
        "expo-splash-screen",
        {
          "backgroundColor": "#F7F8FA",
          "image": "./src/assets/logo.png",
          "imageWidth": 250,
          "dark": {
            "backgroundColor": "#151a25ff",
            "image": "./src/assets/logo.png"
          }
        }
      ]
    ]
  }
}
