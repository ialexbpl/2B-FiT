{
  "expo": {
    "name": "2B FiT",
    "slug": "2b-fit",
    "scheme": "to-be-fit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo.png",
    "updates": {
      "url": "",
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tofitlabs.mobile"
    },
    "android": {
      "package": "com.axel2004b.x2bfit",
      "permissions": [
        "ACTIVITY_RECOGNITION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-font",
      "expo-secure-store",
      "expo-sqlite",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "e5a6fd31-63e7-40da-8ab6-b71dc634b091"
      }
    }
  }
}
